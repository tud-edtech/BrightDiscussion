{
  "manifest_version": 2,
  "name": "Brighstpace Kittenator",
  "version": "0.0.1",
  "description": "Google chrome extension that adds a picture of a kitten after every page in a pdf document on bs.",
  "sandbox": {
    "pages": ["index.html"]
  },
  "content_scripts": [
    {
      "run_at": "document_end",
      "js": [
        "content.js"
      ],
      "all_frames": true,
      "matches": [
        "https://brightspace.tudelft.nl/d2l/le/content/*/viewContent/*/View"
      ]
    }
  ],
  "web_accessible_resources": [
    "*"
  ]
}

